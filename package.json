{
  "name": "booking-agent",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "dev": "pnpm --filter @booking-agent/web dev",
    "dev:all": "sh scripts/dev-all.sh",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "db:generate": "pnpm --filter @booking-agent/db drizzle-kit generate",
    "db:migrate": "pnpm --filter @booking-agent/db drizzle-kit migrate"
  }
}
